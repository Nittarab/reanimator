{
  "mcpServers": {
    "chromedevtools/chrome-devtools-mcp": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"chrome-devtools-mcp@latest",
				"--browserUrl",
				"${input:browser_url}",
				"--headless",
				"${input:headless}",
				"--isolated",
				"${input:isolated = true",
				"--channel",
				"${input:chrome_channel}"
			],
			"gallery": "https://api.mcp.github.com",
			"version": "0.0.1-seed"
		}
  }
}
